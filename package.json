{
    "name": "@darraghor/eslint-plugin-nestjs-typed",
    "version": "1.0.0",
    "description": "Eslint rules for nestjs projects",
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "npm run build",
        "build": "npm run clean && mkdir ./dist && tsc --project tsconfig.build.json",
        "lint": "npx eslint --no-error-on-unmatched-pattern",
        "test": "vitest run",
        "test:watch": "npm run test -- --watch",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release"
    },
    "release": {
        "branches": [
            "main",
            {
                "name": "release/*",
                "prerelease": "rc",
                "channel": "next"
            }
        ]
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "homepage": "https://github.com/darraghoriordan/eslint-plugin-nestjs-typed",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/eslint-plugin-nestjs-typed"
    },
    "engines": {
        "node": "^18.18.0 || >=20.0.0"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "@typescript-eslint/scope-manager": "^8.50.0",
        "@typescript-eslint/type-utils": "^8.50.0",
        "@typescript-eslint/utils": "^8.50.0",
        "eslint-module-utils": "2.12.1",
        "glob": "13.0.0",
        "reflect-metadata": "0.2.2",
        "ts-api-utils": "2.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "20.2.0",
        "@commitlint/config-conventional": "20.2.0",
        "@eslint/js": "9.39.2",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "7.1.0",
        "@types/eslint": "9.6.1",
        "@types/jest": "30.0.0",
        "@types/node": "25.0.2",
        "@typescript-eslint/eslint-plugin": "8.50.0",
        "@typescript-eslint/parser": "8.50.0",
        "@typescript-eslint/rule-tester": "8.50.0",
        "class-validator": "0.14.3",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-unicorn": "62.0.0",
        "globals": "16.5.0",
        "husky": "9.1.7",
        "jest": "30.2.0",
        "jest-create-mock-instance": "2.0.0",
        "jest-junit": "16.0.0",
        "lint-staged": "16.2.7",
        "prettier": "3.7.4",
        "semantic-release": "25.0.2",
        "ts-jest": "29.4.6",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "typescript-eslint": "8.50.0",
        "vite": "7.3.0",
        "vitest": "4.0.15"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": "^7.0.0 || ^8.0.0",
        "class-validator": "*",
        "eslint": ">=9.18.0"
    },
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "nestjs",
        "linter",
        "lint",
        "style"
    ],
    "packageManager": "pnpm@10.25.0",
    "license": "ISC"
}
